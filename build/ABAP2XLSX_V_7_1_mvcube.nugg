  lo_element_root-&gt;set_attribute_ns( name  = &apos;xmlns&apos;
                                     value = lc_xml_node_rels_ns ).

**********************************************************************
* STEP 4: Create subnodes
  &quot; Theme node
  lo_element = lo_document-&gt;create_simple_element( name   = lc_xml_node_relationship
