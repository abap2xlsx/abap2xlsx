*      iv_syscolorlast =
  ).
     lo_theme-&gt;set_color(
    exporting
      iv_type         = zcl_excel_theme_color_scheme=&gt;c_light1
      iv_srgb         = &apos;778899&apos;
*      iv_syscolorname =
*      iv_syscolorlast =
  ).

     lo_theme-&gt;set_color(
    exporting
      iv_type         = zcl_excel_theme_color_scheme=&gt;c_light1
      iv_srgb         = &apos;9932CC&apos;
