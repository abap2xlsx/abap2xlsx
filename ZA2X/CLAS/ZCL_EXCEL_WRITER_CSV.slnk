 <method CLSNAME="ZCL_EXCEL_WRITER_CSV" CMPNAME="SET_DECIMAL" VERSION="1" LANGU="D" DESCRIPT="Set the Decimal Point Character" EXPOSURE="2" STATE="1" EDITORDER="5 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
  <parameter CLSNAME="ZCL_EXCEL_WRITER_CSV" CMPNAME="SET_DECIMAL" SCONAME="IP_VALUE" VERSION="1" LANGU="D" DESCRIPT="Decimal Point" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="CHAR01" PAROPTIONL="X"/>
  <source>method SET_DECIMAL.
  DECIMAL = IP_VALUE.
  if DECIMAL is initial.
*
*   Decimal point from user info
*
    select single DCPFM from  USR01
                        into  DECIMAL
                        where BNAME = SY-UNAME.
    translate DECIMAL using &apos;X. ,Y,&apos;.
  endif.
endmethod.</source>
 </method>
 <method CLSNAME="ZCL_EXCEL_WRITER_CSV" CMPNAME="SET_DELIMITER" VERSION="1" LANGU="D" DESCRIPT="Set Delimiter value" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
  <parameter CLSNAME="ZCL_EXCEL_WRITER_CSV" CMPNAME="SET_DELIMITER" SCONAME="IP_VALUE" VERSION="1" LANGU="D" DESCRIPT="Delimiter Value" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="CHAR01" PARVALUE="&apos;;&apos;"/>
  <source>method SET_DELIMITER.
  delimiter = ip_value.
  endmethod.</source>
 </method>
 <method CLSNAME="ZCL_EXCEL_WRITER_CSV" CMPNAME="SET_ENCLOSURE" VERSION="1" LANGU="D" DESCRIPT="Set Enclosure character" EXPOSURE="2" STATE="1" EDITORDER="3 " DISPID="0 " MTDTYPE="0" MTDDECLTYP="1" BCMTDCAT="00" BCMTDSYN="0">
  <parameter CLSNAME="ZCL_EXCEL_WRITER_CSV" CMPNAME="SET_ENCLOSURE" SCONAME="IP_VALUE" VERSION="1" LANGU="D" DESCRIPT="Text field enclosure character" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " PARDECLTYP="0" PARPASSTYP="0" TYPTYPE="1" TYPE="CHAR01" PARVALUE="&apos;&quot;&apos;"/>
  <source>method SET_ENCLOSURE.
