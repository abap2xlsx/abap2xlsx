        lc_xml_node_rId1_id       TYPE string VALUE &apos;rId1&apos;,
        lc_xml_node_rId2_id       TYPE string VALUE &apos;rId2&apos;,
        lc_xml_node_rId3_id       TYPE string VALUE &apos;rId3&apos;,
