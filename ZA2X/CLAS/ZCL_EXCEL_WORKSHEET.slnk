    elseif error_nr eq ret_call_not_flushed.
      &quot;call still not flushed
      call method c_oi_errors=&gt;translate_proxy_error_code
        EXPORTING
          errorcode   = error_nr
          errorstring = me-&gt;param2  &quot;method name
